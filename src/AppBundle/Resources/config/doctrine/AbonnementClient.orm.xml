<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AbonnementClient" table="abonnement_client">
    <indexes>
      <index name="fk_client_abonnement2_idx" columns="id_abonnement"/>
      <index name="IDX_2577B9CAE173B1B8" columns="id_client"/>
    </indexes>
    <id name="dateDebut" type="datetime" column="date_debut"/>
    <id name="idClient" association-key="true"/>
    <id name="idAbonnement" association-key="true"/>
    <field name="nombrePrestationRestante" type="integer" column="nombre_prestation_restante" nullable="false"/>
    <field name="dateFin" type="datetime" column="date_fin" nullable="true"/>
    <one-to-one field="idAbonnement" target-entity="Abonnement">
      <join-columns>
        <join-column name="id_abonnement" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idClient" target-entity="Client">
      <join-columns>
        <join-column name="id_client" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
