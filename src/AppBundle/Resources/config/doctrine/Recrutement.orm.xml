<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Recrutement" table="recrutement">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="true"/>
    <field name="prenom" type="string" column="prenom" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="telephone" type="string" column="telephone" length="255" nullable="true"/>
    <field name="disponibiliteDimanche" type="string" column="disponibilite_dimanche" length="45" nullable="true"/>
    <field name="travailMin20" type="string" column="travail_min_20" length="45" nullable="true"/>
    <field name="age" type="string" column="age" length="45" nullable="true"/>
    <field name="dateEnregistrement" type="datetime" column="date_enregistrement" nullable="true"/>
    <field name="cv" type="blob" column="cv" length="65535" nullable="true"/>
  </entity>
</doctrine-mapping>
