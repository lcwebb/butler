<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdresseLivraison" table="adresse_livraison">
    <indexes>
      <index name="fk_client_adresse_idx" columns="id_client"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adresse" type="text" column="adresse" length="65535" nullable="true"/>
    <field name="cp" type="string" column="cp" length="5" nullable="true"/>
    <field name="ville" type="string" column="ville" length="255" nullable="true"/>
    <many-to-one field="idClient" target-entity="Client">
      <join-columns>
        <join-column name="id_client" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
