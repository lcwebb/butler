<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Recruitment" table="recruitment">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="firstname" type="string" column="firstname" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="phone" type="string" column="phone" length="45" nullable="true"/>
    <field name="availabilitySunday" type="string" column="availability_sunday" length="45" nullable="true"/>
    <field name="availableMoreThan20Hours" type="string" column="available_more_than_20_hours" length="45" nullable="true"/>
    <field name="autoEntrepreneur" type="string" column="auto_entrepreneur" length="45" nullable="true"/>
    <field name="age" type="string" column="age" length="45" nullable="true"/>
    <field name="registrationDate" type="datetime" column="registration_date" nullable="true"/>
    <field name="cv" type="blob" column="cv" length="65535" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <field name="finishedAt" type="datetime" column="finished_at" nullable="true"/>
  </entity>
</doctrine-mapping>
